{
  "name": "react-feature-switch",
  "version": "0.1.5",
  "description": "React component to handle feature toggles",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "codecov": "codecov",
    "coverage": "NODE_ENV=test babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "dev": "npm test -- --watch --watch-extensions js,jsx",
    "lint": "eslint app test",
    "prepublish": "npm run build",
    "start": "NODE_ENV=production ./node_modules/.bin/babel-node index.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --recursive"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^15.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "3.5.0",
    "chai-enzyme": "^0.4.2",
    "codecov": "^1.0.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-standard": "4.4.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "1.3.1",
    "isparta": "4.0.0",
    "jsdom": "^9.0.0",
    "mocha": "2.4.5",
    "react-dom": "^15.0.2"
  }
}
